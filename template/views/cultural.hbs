  <div class="add mb-3">
    <a href="addEvent">
      <button class="btn btn-warning text-white">Add Event <i class="fa-regular fa-plus"></i></button>
    </a>
  </div>

  <div class="container up shadow">
    <div class="container titleAdd">
      <div>
        <h3 class="text-center">UpComing Events &darr;</h3>
      </div>
    </div>
    <div class="row">
      *
      <h3 style="color: red">There Is No UpComing Events.</h3>
    </div>
  </div>

  <!-- ongoingEvent -->
  <div class="container on shadow mt-4">
    <div class="container">
      <div>
        <h3 class="text-center">OnGoing Events &darr;</h3>
      </div>
    </div>
    <div class="row">
      <div class="col-12 col-sm-3 col-lg-3 col-md-6 mainEvents">
        <div class="events">
          <div class="card">
            <img src="./Annual Fest_Image.jpg" class="card-img" alt="..." />
            <div class="card-body">
              <h5 class="card-title">Annual Fest</h5>
              <p class="card-text mt-3">Date : 20/6/2023</p>
              <a href="#" class="btn btn-primary">Let's Visit</a>
            </div>
          </div>
        </div>
      </div>

      <div class="col-12 col-sm-3 col-lg-3 col-md-6 mainEvents">
        <div class="events">
          <div class="card">
            <img src="./Lakshya Image.jpg" class="card-img" alt="..." />
            <div class="card-body">
              <h5 class="card-title">Lakshya 2k23</h5>
              <p class="card-text mt-3">Date : 28/6/2023</p>
              <a href="#" class="btn btn-primary">Let's Visit</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- completed -->
  <div class="container shadow cmp mt-4" style="margin-bottom: 70px;">
    <div class="container titleAdd">
      <div>
        <h3 class="text-center">Completed Events &rarr;</h3>
      </div>
    </div>
    <div class="row">
      *
      <h3 style="color: red">There is No Completed Events.</h3>
    </div>
  </div>



